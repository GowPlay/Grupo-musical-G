<link rel="stylesheet" type="text/css" href="js/rs-plugin/css/settings.css" media="screen">
<link rel="stylesheet" type="text/css" href="/Plantilla/css/isotope.css" media="screen">
<link rel="stylesheet" href="/Plantilla/css/flexslider.css" type="text/css">
<link rel="stylesheet" href="/Plantilla/js/fancybox/jquery.fancybox.css" type="text/css" media="screen">
<link rel="stylesheet" href="/Plantilla/css/bootstrap.css">
<link rel="stylesheet"
  href="https://fonts.googleapis.com/css?family=Noto+Serif:400,400italic,700|Open+Sans:300,400,600,700">

<link rel="stylesheet" href="/Plantilla/css/style.css">
<!-- skin -->
<link rel="stylesheet" href="/Plantilla/skin/default.css">

</head>
{{>navigation_dos}}
<main>

  <!-- section works -->
  <br>
  <br>
  <br>
  <section id="section-works" class="section appear clearfix">
    <div class="container">
      <div class="row mar-bot40">

        <div class="section-header">
          <h2>Disfruta de nuestros mejores exitos </h2>

          <div class="row ">

            {{#each biografia}}
            <div class="col-md-3 mx-auto">
              <center>
                <img src="/img/guitarist-1031087_1920.jpg" alt=" agrupacion Kaliente" width="300px" height="300px">
              </center>
            </div>
            <div class="col-md-9">
              <div class="col-md-4" id="color-direccion">
                <h2>{{origen}}</h2>
                <samp>{{genero}}</samp>
                <p>{{email}}</p>
              </div>
              <div class="col-md-7  " id="color-direccion">
                <div class="card-block">
                  <p> {{parrafo_1}}</p>
                  <hr>
                  <p> {{parrafo_2}}</p>
                </div>
              </div>
            </div>
            {{else}}
            <div class="container">
              <div class="row">
                <div class="col">
                  <h2> NOS ENCONTRAMOS EN MANTENIMIENTO</h2>
                </div>
              </div>
            </div>
            {{/each}}
          </div>
          <hr>
          <div class="row">
            <div class="col-md-12">
              {{#each artistas}}
              <div class="col-md-3">
                <div class="card text-center">
                <div class="card-header" >
                  <img src="{{url}}" alt="{{nombre}}" width="120px" height="120px"  >
                  <h3>{{nombre}}</h3>
                </div>
                <div class="card-body">
                  <div class="form-group">
                    <h3> {{email}}</h3>
                  </div>
                  <div class="form-group">
                    <h3> {{descripcion}}</h3>
                  </div>

                </div>
              </div>
              </div>
              {{/each}}
            </div>
          </div>
        </div>
        <br>
      </div>
    </div>
  </section>


</main>
{{>footer}}